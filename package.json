    "react-bootstrap": "0.28.3",
    "react-codemirror": "0.2.6",
    "react-dom": "0.14.8",
    "react-redux": "4.4.0",
    "react-remarkable": "1.1.1",
    "react-router": "2.0.0",
    "react-router-redux": "3.0.0",
    "react-utils-helper": "0.1.4",
    "redux": "3.3.1",
    "request-promise": "2.0.1",
    "rimraf": "2.5.2",
    "semver": "5.3.0",
    "sequelize-fixtures": "0.4.10",
