{
  "name": "jcatalog-showroom",
  "version": "0.0.13",
  "description": "React components catalog component.",
  "scripts": {
    "lint": "eslint src",
    "start": "nodemon --watch src/server src/server/index --exec babel-node",
    "build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --plugins transform-object-assign,transform-decorators-legacy --ignore __tests__,*.spec.js src/client --out-dir lib",
    "prepublish": "npm run build",
    "link": "webpack --config ./webpack.development.config.js --watch",
    "npm-packages-install": "node ./src/server/npm-installer/make-install.js",
    "npm-packages-bundle": "node ./src/server/npm-bundler/make-bundles.js",
    "npm-packages-scan": "node ./src/server/npm-scanner/make-scan.js",
    "npm-packages-init": "npm run npm-packages-install & npm run npm-packages-bundle & npm run npm-packages-scan"
  },
  "author": {
    "name": "Kirill Volkovich",
    "email": "volkovich@scand.com"
  },
  "main": "lib/bundle.js",
  "files": [
    "lib",
    "README.md"
  ],
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "6.5.1",
    "babel-core": "6.4.0",
    "babel-eslint": "6.0.4",
    "babel-jest": "11.0.2",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.5.2",
    "babel-standalone": "6.14.0",
    "cross-env": "1.0.7",
    "css-loader": "0.21.0",
    "eslint": "2.11.0",
    "eslint-config-jcatalog": "1.0.2",
    "eslint-plugin-react": "5.1.1",
    "express": "4.13.4",
    "file-loader": "^0.9.0",
    "fuzzysearch": "^1.0.3",
    "jcatalog-bootstrap": "0.1.2",
    "jcatalog-i18n": "1.0.0",
    "jquery": "2.2.0",
    "json-loader": "0.5.4",
    "less": "2.6.1",
    "less-loader": "2.2.2",
    "lodash": "4.13.1",
    "markdown-to-ast": "3.4.0",
